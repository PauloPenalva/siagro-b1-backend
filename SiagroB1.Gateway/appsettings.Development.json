{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "SiagroCommon": "Server=localhost;Database=IDX_SIAGRO_COMMON;User Id=sa;Password=IdxSist3m@s;TrustServerCertificate=True;"
  },
  "ReverseProxy": {
    "Routes": {
      "odata-metadata": {
        "ClusterId": "backend",
        "Match": {
          "Path": "/odata/$metadata"
        },
        "AuthorizationPolicy": null
      },
      "odata-route": {
        "ClusterId": "backend",
        "Match": {
          "Path": "/odata/{**catch-all}"
        },
        "AuthorizationPolicy": "AuthenticatedOnly"
      },
      "reports-route": {
        "ClusterId": "reports",
        "Match": {
          "Path": "/reports/{**catch-all}"
        },
        "AuthorizationPolicy": "AuthenticatedOnly"
      }
    },
    "Clusters": {
      "backend": {
        "Destinations": {
          "odata-destination": {
            "Address": "http://localhost:50000/"
          }
        }
      },
      "reports": {
        "Destinations": {
          "reports-destination": {
            "Address": "http://localhost:8081/"
          }
        }
      }
    }
  }
}
